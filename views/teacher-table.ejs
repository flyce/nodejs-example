<h2>Query result</h2>
<p>The query result is shown below. You can click edit, delete or view buttons for further action.</p>
<table class="table">
    <thead>
    <tr>
        <th>Teacher ID</th>
        <th>Name</th>
        <th>Sex</th>
        <th>Course</th>
        <th class="modify">Edit</th>
        <th class="modify delete">Delete</th>
        <th>Detail</th>
    </tr>
    </thead>
    <tbody>
    <% if (userData.length){ %>
        <% userData.forEach((teacher)=>{ %>
            <tr>
                <td><%= teacher.teacherID %></td>
                <td><%= teacher.name %></td>
                <td><%= teacher.sex %></td>
                <td><%= teacher.cName %></td>
                <td class="modify">
                    <button type="button" class="btn btn-primary btn-sm" onclick="showTeacherEditDialog('<%= teacher.teacherID %>','<%= teacher.cName %>')">EDIT</button>
                </td>
                <td class="modify delete">
                    <button type="button" class="btn btn-danger btn-sm" onclick="showTeacherDeleteDialog(<%= teacher.teacherID %>)">DELETE</button>
                </td>
                <td>
                    <form action="/detail" method="post">
                        <input type="text" class="form-control" name="type" value="teacher" hidden>
                        <input type="text" class="form-control" name="id" value="<%= teacher.teacherID %>" hidden>
                        <button type="submit" class="btn btn-primary btn-sm">VIEW</button>
                    </form>
                </td>
            </tr>
    <% })}else{ %>
        <p>Nothing matches your query options.</p>
    <% } %>
    </tbody>
</table>
<script>hideModifyBtn("<%= userType %>")</script>